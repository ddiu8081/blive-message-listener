{
  "name": "blive-message-listener",
  "version": "0.4.8",
  "description": "Bilibili-live danmu listener with type",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/ddiu8081/blive-message-listener#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ddiu8081/blive-message-listener"
  },
  "author": "ddiu8081 <ddiu8081@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "require": "./dist/browser.cjs",
      "import": "./dist/browser.js"
    }
  },
  "files": [
    "dist",
    "browser.d.ts"
  ],
  "keywords": [
    "bilibili",
    "bilibili-live",
    "danmaku",
    "danmu"
  ],
  "sideEffects": false,
  "dependencies": {
    "tiny-bilibili-ws": "^1.0.0"
  },
  "devDependencies": {
    "tsup": "8.2.4",
    "typescript": "5.5.4"
  },
  "packageManager": "pnpm@9.9.0"
}
